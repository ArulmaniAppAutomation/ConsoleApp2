{
  "AppAutomationMenuItem": "Apps",
  "AppAutomationCaseName": "AndroidStoreAppTest",
  "AppAutomationType": "Android store app",
  "Type": "AppAutomation",
  "Name": "AndroidStoreRegression",
  "Scenarios": [
    {
      "Name": "Go to App page and create Windows app(Win32) apps",
      "UtilsName": "PlaywrightTests.Common.Utils.BaseUtils.Apps.ByPlatform.AllAppsUtils",
      "Steps": [
        {
          "Preparation": [
            {
              "ControlType": "GoToMainPageAsync"
            },
            {
              "ControlType": "ClickAddButtonAsync"
            }
          ]
        }
      ]
    },
    {
      "Name": "Select app type",
      "UtilsName": "PlaywrightTests.Common.Utils.BaseUtils.PopUp.SelectAppTypeUtils",
      "Steps": [
        {
          "Preparation": [
            {
              "ControlType": "SelectAppTypeAsync",
              "OperationValue": "Android store app"
            }
          ]
        }
      ]
    },
    {
      "Name": "App information set app information",
      "UtilsName": "PlaywrightTests.Common.Utils.BaseUtils.Apps.ByPlatform.AllAppsUtils",
      "Steps": [
        {
          "Preparation": [
            {
              "ControlType": "SetAppInformationNameAsync",
              "OperationValue": "25533151Generated from TestCaseName"
            },
            {
              "ControlType": "SetAppInformationDescriptionAsync",
              "OperationValue": "UI Automation testing"
            },
            {
              "ControlType": "SetAppInformationPublisherAsync",
              "OperationValue": "AppLifeEx"
            },
            {
              "ControlType": "SetAppInformationAppstoreUrlAsync",
              "OperationValue": "https://play.google.com/store/apps/details?id=com.facebook.mlite"
            },
            {
              "ControlType": "SetMinimumOperationSystemAsync",
              "Value": [ "Android 4.4 (KitKat)" ]
            },
            {
              "ControlType": "SetAppInformationUrlAsync",
              "OperationValue": "https://www.windows.com"
            },
            {
              "ControlType": "SetAppInformationCategoryAsync",
              "Value": [ "Data management", "Business" ]
            },
            {
              "ControlType": "SetShowThisAsAFeaturedAppInTheCompanyPortalAsync",
              "OperationValue": "Yes"
            },
            {
              "ControlType": "ClickAppInformationSelectImageButtonAsync"
            }
          ]
        }
      ]
    },
    {
      "Name": "App information select app package file",
      "UtilsName": "PlaywrightTests.Common.Utils.BaseUtils.PopUp.UploadFileUtils",
      "Steps": [
        {
          "Preparation": [
            {
              "ControlType": "UploadFile",
              "OperationValue": "AppAutomation\\logo1.jpg"
            },
            {
              "ControlType": "ClickOKButtonAsync"
            }
          ]
        }
      ]
    },
    {
      "Name": "App information Click next button",
      "UtilsName": "PlaywrightTests.Common.Utils.BaseUtils.Apps.ByPlatform.AllAppsUtils",
      "Steps": [
        {
          "Preparation": [
            {
              "ControlType": "ClickNextButtonAsync"
            }
          ]
        }
      ]
    },
    {
      "Name": "App information set Assignments",
      "UtilsName": "PlaywrightTests.Common.Utils.BaseUtils.Apps.ByPlatform.AllAppsUtils",
      "Steps": [
        {
          "Preparation": [
            {
              "ControlType": "InitAppAutomationAppAssignment"
            },
            {
              "ControlType": "ClickRequiredAddAllDevicesAsync"
            },
            {
              "ControlType": "ClickRequiredAddAllUsersAsync"
            },
            {
              "ControlType": "ClickRequiredAddGroupAsync",
              "OperationValue": "Admin"
            },
            {
              "ControlType": "ClickAvailableForEnrolledDevicesAddGroupAsync",
              "OperationValue": "IWuser0"
            },
            {
              "ControlType": "MarkAssignmentsCompleteAsync"
            },
            {
              "ControlType": "ClickNextButtonAsync"
            }
          ]
        }
      ]
    },
    {
      "Name": "App information Review and create",
      "UtilsName": "PlaywrightTests.Common.Utils.BaseUtils.Apps.ByPlatform.AllAppsUtils",
      "Steps": [
        {
          "Preparation": [
            {
              "ControlType": "ClickCreateButtonWithoutWaitForUploadAsync"
            }
          ]
        }
      ]
    },
    {
      "Name": "App Verify",
      "UtilsName": "PlaywrightTests.Common.Utils.BaseUtils.Apps.ByPlatform.AllAppsUtils",
      "Steps": [
        {
          "Preparation": [
            {
              "ControlType": "initAppAutomationVerifyResult"
            },
            {
              "ControlType": "VerifyPropertyAsync",
              "Value": [ "Name", "25533151Generated from TestCaseName" ]
            },
            {
              "ControlType": "VerifyPropertyAsync",
              "Value": [ "Description", "UI Automation testing" ]
            },
            {
              "ControlType": "VerifyPropertyAsync",
              "Value": [ "Publisher", "AppLifeEx" ]
            },
            {
              "ControlType": "VerifyPropertyAsync",
              "Value": [
                "Appstore URL",
                "https://play.google.com/store/apps/details?id=com.facebook.mlite"
              ]
            },
            {
              "ControlType": "VerifyPropertyAsync",
              "Value": [
                "Minimum operating system",
                "Android 4.4 (KitKat)"
              ]
            },
            {
              "ControlType": "VerifyPropertyAsync",
              "Value": [
                "Category",
                "Business"
              ]
            },
            {
              "ControlType": "VerifyPropertyAsync",
              "Value": [
                "Category",
                "Data management"
              ]
            },
            {
              "ControlType": "VerifyPropertyAsync",
              "Value": [
                "Show this as a featured app in the Company Portal",
                "Yes"
              ]
            },
            {
              "ControlType": "VerifyPropertyAsync",
              "Value": [
                "Information URL",
                "https://www.windows.com"
              ]
            },
            {
              "ControlType": "VerifyPropertyAssignmentsAsync",
              "Value": [ "Required", "All devices" ]
            },
            {
              "ControlType": "VerifyPropertyAssignmentsAsync",
              "Value": [ "Required", "All users" ]
            },
            {
              "ControlType": "VerifyPropertyAssignmentsAsync",
              "Value": [ "Required", "Admin" ]
            },
            {
              "ControlType": "VerifyPropertyAssignmentsAsync",
              "Value": [ "Available for enrolled devices", "iwuser0" ]
            },
            {
              "ControlType": "SuccessAppAutomationVerifyResult"
            }
          ]
        }
      ]
    },
    {
      "Name": "App information update",
      "UtilsName": "PlaywrightTests.Common.Utils.BaseUtils.Apps.ByPlatform.AllAppsUtils",
      "Steps": [
        {
          "Preparation": [
            {
              "ControlType": "InitAppAutomationAppUpdated"
            },
            {
              "ControlType": "ClickAppInformationEditButtonAsync"
            },
            {
              "ControlType": "SetAppInformationUrlAsync",
              "OperationValue": "https://www.microsoft.com"
            },
            {
              "ControlType": "SetAppInformationDeveloperAsync",
              "OperationValue": "Microsoft"
            },
            {
              "ControlType": "ClickReviewSaveButtonAsync"
            },
            {
              "ControlType": "ClickSaveButtonAsync"
            },
            {
              "ControlType": "SuccessAppAutomationAppUpdated"
            }
          ]
        }
      ]
    },
    {
      "Name": "App information verify",
      "UtilsName": "PlaywrightTests.Common.Utils.BaseUtils.Apps.ByPlatform.AllAppsUtils",
      "Steps": [
        {
          "Preparation": [
            {
              "ControlType": "initAppAutomationVerifyResult"
            },
            {
              "ControlType": "VerifyPropertyAsync",
              "Value": [
                "Information URL",
                "https://www.microsoft.com"
              ]
            },
            {
              "ControlType": "VerifyPropertyAsync",
              "Value": [
                "Developer",
                "Microsoft"
              ]
            },
            {
              "ControlType": "VerifyDeviceInstallStatusAsync"
            },
            {
              "ControlType": "VerifyUserInstallStatusAsync"
            },
            {
              "ControlType": "VerifyMonitorAppInstallStatusAsync"
            },
            {
              "ControlType": "SuccessAppAutomationVerifyResult"
            }
          ]
        }
      ]
    },
    {
      "Name": "App delete",
      "UtilsName": "PlaywrightTests.Common.Utils.BaseUtils.Apps.ByPlatform.AllAppsUtils",
      "Steps": [
        {
          "Preparation": [
            {
              "ControlType": "GoToMainPageAsync"
            },
            {
              "ControlType": "DeleteAppByNameAsync"
            }
          ]
        }
      ]
    }
  ]
}
