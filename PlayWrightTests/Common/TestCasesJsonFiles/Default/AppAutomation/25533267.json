{
  "AppAutomationMenuItem": "Apps",
  "AppAutomationCaseName": "IosStoreAppTest",
  "AppAutomationType": "iOS store app",
  "Type": "AppAutomation",
  "Name": "iOS store app update regression test",
  "Scenarios": [
    {
      "Name": "select app type",
      "UtilsName": "PlaywrightTests.Common.Utils.BaseUtils.Apps.ByPlatform.AllAppsUtils",
      "Steps": [
        {
          "Preparation": [
            {
              "ControlType": "GoToMainPageAsync"
            },
            {
              "ControlType": "ClickAddButtonAsync"
            }
          ]
        }
      ]
    },
    {
      "Name": "Select app type",
      "UtilsName": "PlaywrightTests.Common.Utils.BaseUtils.PopUp.SelectAppTypeUtils",
      "Steps": [
        {
          "Preparation": [
            {
              "ControlType": "SelectAppTypeAsync",
              "OperationValue": "iOS store app"
            }
          ]
        }
      ]
    },
    {
      "Name": "Set App information",
      "UtilsName": "PlaywrightTests.Common.Utils.BaseUtils.Apps.ByPlatform.AllAppsUtils",
      "Steps": [
        {
          "Preparation": [
            {
              "ControlType": "ClickSelectTheAppStoreButtonAsync"
            },
            {
              "ControlType": "SelectTheAppStoreAsync",
              "OperationValue": "YouTube"
            },
            {
              "ControlType": "SetAppInformationNameAsync",
              "OperationValue": "25533267iOS store app update regression test"
            },
            {
              "ControlType": "SetAppInformationDescriptionAsync",
              "OperationValue": "UI Automation testing"
            },
            {
              "ControlType": "ClickNextButtonAsync"
            }
          ]
        }
      ]
    },
    {
      "Name": "Set App Assignments",
      "UtilsName": "PlaywrightTests.Common.Utils.BaseUtils.Apps.ByPlatform.AllAppsUtils",
      "Steps": [
        {
          "Preparation": [
            {
              "ControlType": "ClickAvailableForEnrolledDevicesAllUsersAsync"
            },
            {
              "ControlType": "ClickAvailableForEnrolledDevicesAddGroupAsync",
              "OperationValue": "Admin"
            },
            {
              "ControlType": "ClickNextButtonAsync"
            },
            {
              "ControlType": "ClickCreateButtonWithoutWaitForUploadAsync"
            }
          ]
        }
      ]
    },
    {
      "Name": "App Verify",
      "UtilsName": "PlaywrightTests.Common.Utils.BaseUtils.Apps.ByPlatform.AllAppsUtils",
      "Steps": [
        {
          "Preparation": [
            {
              "ControlType": "initAppAutomationVerifyResult"
            },
            {
              "ControlType": "VerifyPropertyAsync",
              "Value": [ "Name", "25533267iOS store app update regression test" ]
            },
            {
              "ControlType": "VerifyPropertyAsync",
              "Value": [ "Description", "UI Automation testing" ]
            },
            {
              "ControlType": "VerifyPropertyAssignmentsAsync",
              "Value": [ "Available for enrolled devices", "All users" ]
            },
            {
              "ControlType": "VerifyPropertyAssignmentsAsync",
              "Value": [ "Available for enrolled devices", "Admin" ]
            },
            {
              "ControlType": "SuccessAppAutomationVerifyResult"
            }
          ]
        }
      ]
    },
    {
      "Name": "Update app information",
      "UtilsName": "PlaywrightTests.Common.Utils.BaseUtils.Apps.ByPlatform.AllAppsUtils",
      "Steps": [
        {
          "Preparation": [
            {
              "ControlType": "InitAppAutomationAppUpdated"
            },
            {
              "ControlType": "ClickAppInformationEditButtonAsync"
            },
            {
              "ControlType": "SetAppInformationDescriptionAsync",
              "OperationValue": "Changed: UI Automation testing"
            },
            {
              "ControlType": "SetAppInformationPublisherAsync",
              "OperationValue": "Changed: AppLifeEx"
            },
            {
              "ControlType": "SetShowThisAsAFeaturedAppInTheCompanyPortalAsync",
              "OperationValue": "Yes"
            },
            {
              "ControlType": "SetMinimumOperationSystemAsync",
              "Value": [ "iOS 10.0" ]
            },
            {
              "ControlType": "SetAppInformationCategoryAsync",
              "Value": [ "Data management" ]
            },
            {
              "ControlType": "SetAppInformationUrlAsync",
              "OperationValue": "http://info.url"
            },
            {
              "ControlType": "SetAppInformationPrivacyURLAsync",
              "OperationValue": "https://privacy.url"
            },
            {
              "ControlType": "SetAppInformationDeveloperAsync",
              "OperationValue": "Changed: AppLifeEx Developer Field"
            },
            {
              "ControlType": "SetAppInformationOwnerAsync",
              "OperationValue": "Changed: AppLifeEx Owner Field"
            },
            {
              "ControlType": "SetAppInformationNotesAsync",
              "OperationValue": "Changed: AppLifeEx Notes Field"
            },
            {
              "ControlType": "ClickAppInformationChangeImageButtonAsync"
            }
          ]
        }
      ]
    },
    {
      "Name": "App information select app package file",
      "UtilsName": "PlaywrightTests.Common.Utils.BaseUtils.PopUp.UploadFileUtils",
      "Steps": [
        {
          "Preparation": [
            {
              "ControlType": "UploadFile",
              "OperationValue": "AppAutomation\\logo1.jpg"
            },
            {
              "ControlType": "ClickOKButtonAsync"
            }
          ]
        }
      ]
    },
    {
      "Name": "Update App information",
      "UtilsName": "PlaywrightTests.Common.Utils.BaseUtils.Apps.ByPlatform.AllAppsUtils",
      "Steps": [
        {
          "Preparation": [
            {
              "ControlType": "ClickReviewSaveButtonAsync"
            },
            {
              "ControlType": "ClickSaveButtonAsync"
            },
            {
              "ControlType": "SuccessAppAutomationAppUpdated"
            }
          ]
        }
      ]
    },
    {
      "Name": "App Verify",
      "UtilsName": "PlaywrightTests.Common.Utils.BaseUtils.Apps.ByPlatform.AllAppsUtils",
      "Steps": [
        {
          "Preparation": [
            {
              "ControlType": "initAppAutomationVerifyResult"
            },
            {
              "ControlType": "VerifyPropertyAsync",
              "Value": [ "Name", "25533267iOS store app update regression test" ]
            },
            {
              "ControlType": "VerifyPropertyAsync",
              "Value": [ "Description", "Changed: UI Automation testing" ]
            },
            {
              "ControlType": "VerifyPropertyAsync",
              "Value": [ "Publisher", "Changed: AppLifeEx" ]
            },
            {
              "ControlType": "VerifyPropertyAsync",
              "Value": [ "Show this as a featured app in the Company Portal", "Yes" ]
            },
            {
              "ControlType": "VerifyPropertyAsync",
              "Value": [ "Minimum operating system", "iOS 10.0" ]
            },
            {
              "ControlType": "VerifyPropertyAsync",
              "Value": [ "Category", "Data management" ]
            },
            {
              "ControlType": "VerifyPropertyAsync",
              "Value": [
                "Information URL",
                "http://info.url"
              ]
            },
            {
              "ControlType": "VerifyPropertyAsync",
              "Value": [ "Privacy URL", "https://privacy.url" ]
            },
            {
              "ControlType": "VerifyPropertyAsync",
              "Value": [ "Developer", "Changed: AppLifeEx Developer Field" ]
            },
            {
              "ControlType": "VerifyPropertyAsync",
              "Value": [ "Owner", "Changed: AppLifeEx Owner Field" ]
            },
            {
              "ControlType": "VerifyPropertyAsync",
              "Value": [ "Notes", "Changed: AppLifeEx Notes Field" ]
            },
            {
              "ControlType": "VerifyDeviceInstallStatusAsync"
            },
            {
              "ControlType": "VerifyUserInstallStatusAsync"
            },
            {
              "ControlType": "VerifyMonitorAppInstallStatusAsync"
            },
            {
              "ControlType": "SuccessAppAutomationVerifyResult"
            }
          ]
        }
      ]
    },
    {
      "Name": "App delete",
      "UtilsName": "PlaywrightTests.Common.Utils.BaseUtils.Apps.ByPlatform.AllAppsUtils",
      "Steps": [
        {
          "Preparation": [
            {
              "ControlType": "GoToMainPageAsync"
            },
            {
              "ControlType": "DeleteAppByNameAsync"
            }
          ]
        }
      ]
    }
  ]
}
